<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ModeFragmentMain">
        <item name="android:background">@color/black</item>
    </style>
    <style name="ModeFragment">
        <item name="android:background">@color/black</item>
    </style>
    <style name="TextStyleMedium500">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
    </style>
    <style name="TextStyleRegular400">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
    </style>
    <style name="button_activity_main" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_0</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginHorizontal">@dimen/dp_16</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="cornerRadius">@dimen/dp_16</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">@dimen/text_22sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="iconGravity">textStart</item>
        <item name="iconPadding">@dimen/dp_8</item>
        <item name="iconTint">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
    </style>
    <style name="button_activity_settings">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_61</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_16sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="thumbTint">@color/switch_thumb_selector</item>
        <item name="trackTint">@color/switch_track_selector</item>
        <item name="android:drawableTint">@color/white</item>
        <item name="android:paddingStart">@dimen/dp_16</item>
        <item name="android:paddingEnd">@dimen/dp_12</item>
    </style>
    <style name="toolbar">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">@dimen/text_22sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="tint">@color/white</item>
    </style>
    <style name="edit_text">
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/white</item>
        <item name="tint">@color/black</item>
        <item name="android:drawableTint">@color/black</item>
    </style>
    <style name="track_text">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_16sp</item>
    </style>
    <style name="artist_text">
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_11sp</item>
        <item name="android:tint">@color/white</item>
    </style>
    <style name="media_buttons">
        <item name="cornerRadius">@dimen/dp_54</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textSize">@dimen/text_14sp</item>
        <item name="android:letterSpacing">0</item>
        <item name="android:insetTop">@dimen/dp_0</item>
        <item name="android:insetBottom">@dimen/dp_0</item>
    </style>
    <style name="error_image_view">
        <item name="android:src">@drawable/ic_errors_dark_120</item>
    </style>
    <style name="internet_image_view">
        <item name="android:src">@drawable/ic_internet_dark_120</item>
    </style>
    <style name="text_placeholder">
        <item name="android:layout_marginHorizontal">@dimen/dp_24</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_19sp</item>
        <item name="android:textFontWeight">400</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="tv_search_history">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_19sp</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="track_description_media">
        <item name="android:layout_marginStart">@dimen/dp_16</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:fontWeight">400</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="button_play">
        <item name="backgroundTint">@color/black</item>
        <item name="android:tint">@color/white</item>
    </style>
    <style name="text_media">
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textFontWeight">400</item>
    </style>
    <style name="custom_tab_layout" parent="Widget.MaterialComponents.TabLayout">
        <item name="tabBackground">@color/black</item>
        <item name="tabTextColor">@color/white</item>
        <item name="tabIndicatorColor">@color/white</item>
    </style>
    <style name="media_text_placeholder">
        <item name="android:layout_marginHorizontal">@dimen/dp_24</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_19sp</item>
        <item name="android:textFontWeight">500</item>
        <item name="android:textAlignment">center</item>
    </style>
</resources>